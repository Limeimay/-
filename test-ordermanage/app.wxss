/**app.wxss**/
.container { 
  display: flex;

  box-sizing: border-box;
} 
.height100{
  height: 100rpx;
}
.dingdanicon{
  width: 36rpx;
  height: 36rpx;
}
.borderupdown{
  border-top: 1px solid black;
  border-bottom:1px solid black;
}
.borderdown{
  border-bottom:1px solid black;
}
.nexticon{
  height: 40rpx;
  width: 20rpx;
  margin-right: 40rpx;
}
 .flex-row{
  flex-direction: row;
} 
 .flex-column{
  flex-direction: column
} 
.flex-center{
  justify-content: center;
}
.search_view_show {
  display: block;
  color: #808080;
  padding-left: 285rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  font-size: 18rpx;
  background-color: #f3f3f3;
  border-radius: 12rpx;
}

.search {
  margin-top: 100rpx;
  background-color: #f3f3f3;
  border-radius: 12rpx;
  height: 60rpx;
}